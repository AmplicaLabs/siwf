{
  "name": "siwf-all",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "pnpm run --stream -r build",
    "build:utils": "pnpm run --filter @AmplicaLabs/siwf-utils -r build",
    "build:siwf": "pnpm run --filter @AmplicaLabs/siwf -r build",
    "dev": "pnpm --filter @AmplicaLabs/siwf-ui run dev",
    "lint": "pnpm run --stream -r lint",
    "lint:app": "pnpm run --filter @AmplicaLabs/siwf-ui -r lint",
    "lint:utils": "pnpm run --filter @AmplicaLabs/siwf-utils -r lint",
    "format:all": "pnpm run --stream -r format",
    "format": "prettier --plugin-search-dir . --write .",
    "format:check": "prettier --plugin-search-dir . --check .",
    "test": "pnpm run --stream -r test",
    "test:unit": "pnpm run --stream -r test:unit",
    "proxy-wallet": "pnpm --filter @AmplicaLabs/siwf-ui",
    "utils": "pnpm --filter @AmplicaLabs/siwf-utils",
    "chain-setup": "pnpm --filter setup run start"
  },
  "devDependencies": {
    "@AmplicaLabs/siwf-eslint-config": "workspace:*",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "vitest": "^1.4.0"
  },
  "keywords": [],
  "author": "Amplica Labs",
  "license": "Apache-2.0"
}
